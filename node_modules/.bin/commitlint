#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/bigger/Desktop/zhangbin/lesson/ssr-react/node_modules/.pnpm/@commitlint+cli@17.1.2/node_modules/@commitlint/cli/node_modules:/Users/bigger/Desktop/zhangbin/lesson/ssr-react/node_modules/.pnpm/@commitlint+cli@17.1.2/node_modules/@commitlint/node_modules:/Users/bigger/Desktop/zhangbin/lesson/ssr-react/node_modules/.pnpm/@commitlint+cli@17.1.2/node_modules:/Users/bigger/Desktop/zhangbin/lesson/ssr-react/node_modules/.pnpm/node_modules:/Users/bigger/Desktop/zhangbin/lesson/ssr-react/node_modules:/Users/bigger/Desktop/zhangbin/lesson/node_modules:/Users/bigger/Desktop/zhangbin/node_modules:/Users/bigger/Desktop/node_modules:/Users/bigger/node_modules:/Users/node_modules:/node_modules:/Users/bigger/Desktop/zhangbin/lesson/ssr-react/node_modules/@commitlint/cli/node_modules:/Users/bigger/Desktop/zhangbin/lesson/ssr-react/node_modules/@commitlint/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/bigger/Desktop/zhangbin/lesson/ssr-react/node_modules/.pnpm/@commitlint+cli@17.1.2/node_modules/@commitlint/cli/node_modules:/Users/bigger/Desktop/zhangbin/lesson/ssr-react/node_modules/.pnpm/@commitlint+cli@17.1.2/node_modules/@commitlint/node_modules:/Users/bigger/Desktop/zhangbin/lesson/ssr-react/node_modules/.pnpm/@commitlint+cli@17.1.2/node_modules:/Users/bigger/Desktop/zhangbin/lesson/ssr-react/node_modules/.pnpm/node_modules:/Users/bigger/Desktop/zhangbin/lesson/ssr-react/node_modules:/Users/bigger/Desktop/zhangbin/lesson/node_modules:/Users/bigger/Desktop/zhangbin/node_modules:/Users/bigger/Desktop/node_modules:/Users/bigger/node_modules:/Users/node_modules:/node_modules:/Users/bigger/Desktop/zhangbin/lesson/ssr-react/node_modules/@commitlint/cli/node_modules:/Users/bigger/Desktop/zhangbin/lesson/ssr-react/node_modules/@commitlint/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@commitlint/cli/cli.js" "$@"
else
  exec node  "$basedir/../@commitlint/cli/cli.js" "$@"
fi
