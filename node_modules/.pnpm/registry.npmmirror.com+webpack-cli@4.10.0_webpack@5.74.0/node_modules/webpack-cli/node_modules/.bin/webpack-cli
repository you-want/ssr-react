#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/bigger/Desktop/zhangbin/lesson/ssr-react/node_modules/.pnpm/registry.npmmirror.com+webpack-cli@4.10.0_webpack@5.74.0/node_modules/webpack-cli/bin/node_modules:/Users/bigger/Desktop/zhangbin/lesson/ssr-react/node_modules/.pnpm/registry.npmmirror.com+webpack-cli@4.10.0_webpack@5.74.0/node_modules/webpack-cli/node_modules:/Users/bigger/Desktop/zhangbin/lesson/ssr-react/node_modules/.pnpm/registry.npmmirror.com+webpack-cli@4.10.0_webpack@5.74.0/node_modules:/Users/bigger/Desktop/zhangbin/lesson/ssr-react/node_modules/.pnpm/node_modules:/Users/bigger/Desktop/zhangbin/lesson/ssr-react/node_modules:/Users/bigger/Desktop/zhangbin/lesson/node_modules:/Users/bigger/Desktop/zhangbin/node_modules:/Users/bigger/Desktop/node_modules:/Users/bigger/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/bigger/Desktop/zhangbin/lesson/ssr-react/node_modules/.pnpm/registry.npmmirror.com+webpack-cli@4.10.0_webpack@5.74.0/node_modules/webpack-cli/bin/node_modules:/Users/bigger/Desktop/zhangbin/lesson/ssr-react/node_modules/.pnpm/registry.npmmirror.com+webpack-cli@4.10.0_webpack@5.74.0/node_modules/webpack-cli/node_modules:/Users/bigger/Desktop/zhangbin/lesson/ssr-react/node_modules/.pnpm/registry.npmmirror.com+webpack-cli@4.10.0_webpack@5.74.0/node_modules:/Users/bigger/Desktop/zhangbin/lesson/ssr-react/node_modules/.pnpm/node_modules:/Users/bigger/Desktop/zhangbin/lesson/ssr-react/node_modules:/Users/bigger/Desktop/zhangbin/lesson/node_modules:/Users/bigger/Desktop/zhangbin/node_modules:/Users/bigger/Desktop/node_modules:/Users/bigger/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.js" "$@"
else
  exec node  "$basedir/../../bin/cli.js" "$@"
fi
