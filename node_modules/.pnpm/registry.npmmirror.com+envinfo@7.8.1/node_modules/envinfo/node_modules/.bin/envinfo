#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/bigger/Desktop/zhangbin/lesson/ssr-react/node_modules/.pnpm/registry.npmmirror.com+envinfo@7.8.1/node_modules/envinfo/dist/node_modules:/Users/bigger/Desktop/zhangbin/lesson/ssr-react/node_modules/.pnpm/registry.npmmirror.com+envinfo@7.8.1/node_modules/envinfo/node_modules:/Users/bigger/Desktop/zhangbin/lesson/ssr-react/node_modules/.pnpm/registry.npmmirror.com+envinfo@7.8.1/node_modules:/Users/bigger/Desktop/zhangbin/lesson/ssr-react/node_modules/.pnpm/node_modules:/Users/bigger/Desktop/zhangbin/lesson/ssr-react/node_modules:/Users/bigger/Desktop/zhangbin/lesson/node_modules:/Users/bigger/Desktop/zhangbin/node_modules:/Users/bigger/Desktop/node_modules:/Users/bigger/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/bigger/Desktop/zhangbin/lesson/ssr-react/node_modules/.pnpm/registry.npmmirror.com+envinfo@7.8.1/node_modules/envinfo/dist/node_modules:/Users/bigger/Desktop/zhangbin/lesson/ssr-react/node_modules/.pnpm/registry.npmmirror.com+envinfo@7.8.1/node_modules/envinfo/node_modules:/Users/bigger/Desktop/zhangbin/lesson/ssr-react/node_modules/.pnpm/registry.npmmirror.com+envinfo@7.8.1/node_modules:/Users/bigger/Desktop/zhangbin/lesson/ssr-react/node_modules/.pnpm/node_modules:/Users/bigger/Desktop/zhangbin/lesson/ssr-react/node_modules:/Users/bigger/Desktop/zhangbin/lesson/node_modules:/Users/bigger/Desktop/zhangbin/node_modules:/Users/bigger/Desktop/node_modules:/Users/bigger/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cli.js" "$@"
else
  exec node  "$basedir/../../dist/cli.js" "$@"
fi
